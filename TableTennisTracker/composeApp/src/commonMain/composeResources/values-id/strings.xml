	<!-- Help -->
	<string name="help_icon_content_description">Tampilkan bantuan</string>
	<string name="help_rpe_title">Apa itu RPE?</string>
	<string name="help_rpe_description">RPE (Rate of Perceived Exertion) mengukur seberapa berat tubuh Anda terasa bekerja saat berolahraga.</string>
	<string name="help_rpe_scale_intro">Skala 1-10 membantu Anda melacak intensitas latihan:</string>
	<string name="help_rpe_level_1_2">1-2: Sangat mudah - Aktivitas ringan, bisa berbicara dengan mudah</string>
	<string name="help_rpe_level_3_4">3-4: Mudah - Tempo nyaman, bisa berbincang</string>
	<string name="help_rpe_level_5_6">5-6: Sedang - Napas lebih berat, ada tantangan</string>
	<string name="help_rpe_level_7_8">7-8: Sulit - Sulit berbicara, mendorong diri</string>
	<string name="help_rpe_level_9_10">9-10: Usaha maksimal - Intensitas penuh, tidak bisa bertahan</string>
	<string name="help_rpe_tip">Tips: Sebagian besar sesi latihan sebaiknya berada di kisaran 5-7.</string>

